 <%= form_with model: @user, local: true, html: {multipart: true} do |f| %>
 
    <%= f.label :name %>
    <br>
    <%= f.text_field :name %>
    <br><br>
    
    <%= f.label :city %>
    <br>
    <%= f.text_field :city %>
    <br><br>
    
    <%= f.label :suburb %>
    <br>
    <%= f.text_field :suburb %>
    <br><br>
  
    <%= f.label :photo_url %>
    <br>
    <%= f.file_field :photo_url %>
    <br><br>


    <%= f.submit %>

<% end %>